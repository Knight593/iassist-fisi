<?xml version="1.0" encoding="UTF-8"?>
<faces-config xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_1.xsd"
	version="2.1">
	<application>
		<el-resolver>org.springframework.web.jsf.el.SpringBeanFacesELResolver</el-resolver>
	</application>
	<navigation-rule>
		<display-name>tutorLogin.xhtml</display-name>
		<from-view-id>/tutorLogin.xhtml</from-view-id>
		<navigation-case>
			<to-view-id>/tutorPage.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>views/home.xhtml</display-name>
		<from-view-id>/views/home.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>goProyectosRealizados</from-outcome>
			<to-view-id>/views/uiCuerpoProyectosRealizados.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>views/uiCuerpoProyectosRealizados.xhtml</display-name>
		<from-view-id>/views/uiCuerpoProyectosRealizados.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>goHome</from-outcome>
			<to-view-id>/views/home.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>views/uiCuerpoPresentacion.xhtml</display-name>
		<from-view-id>/views/uiCuerpoPresentacion.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>goHome</from-outcome>
			<to-view-id>/views/home.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>views/home.xhtml</display-name>
		<from-view-id>/views/home.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>goPresentacion</from-outcome>
			<to-view-id>/views/uiCuerpoPresentacion.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>views/uiCuerpoPresentacion.xhtml</display-name>
		<from-view-id>/views/uiCuerpoPresentacion.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>goProyectosRealizados</from-outcome>
			<to-view-id>/views/uiCuerpoProyectosRealizados.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>views/uiCuerpoProyectosRealizados.xhtml</display-name>
		<from-view-id>/views/uiCuerpoProyectosRealizados.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>goPresentacion</from-outcome>
			<to-view-id>/views/uiCuerpoPresentacion.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
</faces-config>
